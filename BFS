vector<int> vis;
vector<vector<int>> adj;
void bfs(int x){
    vis[x] = 1;
    queue<int> q;
    q.push(x);
    while(!q.empty()){
        int s = q.front();
        q.pop();
        for(auto u : adj[s]){
            if(vis[u]) continue;
            vis[u] = 1;
            q.push(u);
        }
    }
}
